public class PrintTenNumbers{
    public static void main(String[] args) {

        System.out.println("Numbers 1 to 10 : ");
        for(int i = 1; i <= 10; i++)            //Print 1 to 10
            System.out.println(i);

        System.out.println("");

        System.out.println("Numbers 10 to 1 : ");  //Print 10 to 1
        for(int i = 10; i > 0; i--)
            System.out.println(i);
    }
}